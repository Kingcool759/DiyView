<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".show.Show4"
    tools:ignore="MissingClass,MissingConstraints">

    <com.example.diyview.viewgroup.ring.FiveRingsViewGroup
        android:layout_width="match_parent"
        android:layout_height="200dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_centerInParent="true">

        <com.example.diyview.viewgroup.ring.ColorRingView
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:ring_color="#3F51B5"
            app:ring_width="10" />

        <com.example.diyview.viewgroup.ring.ColorRingView
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:ring_color="#000000"
            app:ring_width="10" />

        <com.example.diyview.viewgroup.ring.ColorRingView
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:ring_color="#ff3c00"
            app:ring_width="10" />

        <com.example.diyview.viewgroup.ring.ColorRingView
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:ring_color="#fcb840"
            app:ring_width="10" />

        <com.example.diyview.viewgroup.ring.ColorRingView
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:ring_color="#24ff00"
            app:ring_width="10" />

    </com.example.diyview.viewgroup.ring.FiveRingsViewGroup>
</androidx.constraintlayout.widget.ConstraintLayout>